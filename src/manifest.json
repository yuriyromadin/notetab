{
  "name": "Note tab v2",
  "version": "1.0.0",
  "manifest_version": 2,
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "permissions": [
      "storage",
      "tabs",
      "activeTab"
  ],
  // "browser_action": {
  //   "default_icon": "static/icons/icon-16.png",
  //   "default_popup": "popup.html"
  // },
  "icons": {
    "16": "static/icon-16.png",
    "48": "static/icon-48.png",
    "128": "static/icon-128.png"
  }
}
